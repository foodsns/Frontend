{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "indent": "off"
    }
}
